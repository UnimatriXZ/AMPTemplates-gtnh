{
  "Name": "GregTech New Horizons",
  "Author": "Lancelite",
  "Target": "GenericModule",
  "DeploymentData": {
    "Executable": "java",
    "Arguments": [
      "-Xms20G",
      "-Xmx20G",
      "-XX:+UseG1GC",
      "-Dfml.readTimeout=180",
      "@java9args.txt",
      "-jar",
      "lwjgl3ify-forgePatches.jar",
      "nogui"
    ],
    "WorkingDirectory": "${RootDirectory}/serverfiles"
  },
  "ConfigManifest": {
    "Port": {
      "type": "int",
      "default": 25576
    },
    "RCONPort": {
      "type": "int",
      "default": 25575
    }
  }
}
